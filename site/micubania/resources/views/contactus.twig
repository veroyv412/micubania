    <section>
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-md-5">
                    <h4 class="uppercase">Contactenos</h4>
                    <hr>
                    <p>
                        438 Marine Parade
                        <br /> Elwood, Victoria
                        <br /> P.O Box 3184
                    </p>
                    <hr>
                    <p>
                        <strong>E:</strong> hello@foundry.net
                        <br />
                        <strong>P:</strong> +614 3948 2726
                        <br />
                    </p>
